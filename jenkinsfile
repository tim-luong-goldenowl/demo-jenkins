pipeline {
  agent {
    label 'linux-slave-01'
  }

  stages {
    stage('try check') {
      steps {
        script {
          def version = readFile "${env.WORKSPACE}/demo.log"
          echo('hahaae')
          publishChecks name: 'Rubocop', title: 'Rubocop', summary: 'Rubocop',
                        text: "${version}",
                        detailsURL: "${env.RUN_DISPLAY_URL}"
        }
      }
    }
  }
}